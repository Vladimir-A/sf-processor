{
  "pipeline":[
    {
     "processor": "sysflowreader",
     "handler": "flattener",
     "in": "sysflow sysflowchan",
     "out": "flat flattenerchan"
    },
    {
     "processor": "policyengine",
     "in": "flat flattenerchan",
     "out": "evt eventchan",
     "policies": "../plugins/actions/hash/policy.yaml", 
     "mode": "enrich"     
    },
    {
     "processor": "exporter",
     "in": "evt eventchan",
     "export": "terminal",
     "format": "ecs"
    }
  ]
}
